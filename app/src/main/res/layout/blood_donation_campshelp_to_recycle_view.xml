<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/inquiry"
        android:layout_width="398dp"
        android:layout_height="312dp"
        android:layout_marginTop="28dp"
        android:background="	#880808"
        android:radius="20dp"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="314dp"
        android:layout_height="49dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="44dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/inquiry"
        app:layout_constraintStart_toStartOf="@+id/inquiry" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="316dp"
        android:layout_height="49dp"
        android:layout_marginBottom="24dp"
        android:background="@color/white"

        app:layout_constraintBottom_toTopOf="@+id/textView21"
        app:layout_constraintEnd_toEndOf="@+id/inquiry"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="@+id/inquiry" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="313dp"
        android:layout_height="49dp"
        android:background="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="@+id/inquiry"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="@+id/inquiry"
        app:layout_constraintTop_toTopOf="@+id/inquiry"
        app:layout_constraintVertical_bias="0.72" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="353dp"
        android:layout_height="49dp"
        android:text="Organization ,Date, Venue"
        android:textAlignment="center"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.535" />

</androidx.constraintlayout.widget.ConstraintLayout>